        "postcss-import": "^11.1.0",
        "postcss-nesting": "^5.0.0",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "tailwindcss": ">=0.5.3",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10"
    }
